    <script>
        // Color palette
        const colors = {
            primary: '#2c5aa0',
            secondary: '#4CAF50',
            accent: '#FF6B35',
            light: '#e8f4f8',
            grade9: '#2E8B57',
            grade10: '#4169E1',
            grade11: '#DC143C',
            grade12: '#FF8C00',
            other: '#708090'
        };

        // Chart 1: Enrollment Trends 2024 vs 2025 (CORRECTED DATA)
        const ctx1 = document.getElementById('enrollmentChart').getContext('2d');
        new Chart(ctx1, {
            type: 'bar',
            data: {
                labels: ['Grade 6 & Below', 'Grade 7', 'Grade 8', 'Grade 9', 'Grade 10', 'Grade 11', 'Grade 12'],
                datasets: [{
                    label: '2024 Enrollments',
                    data: [6.6, 8.1, 10.4, 21.2, 22.1, 21.7, 9.9],
                    backgroundColor: colors.primary + '80',
                    borderColor: colors.primary,
                    borderWidth: 1
                }, {
                    label: '2025 Enrollments',
                    data: [6.5, 6.7, 13.3, 21.0, 25.0, 19.9, 7.6],
                    backgroundColor: colors.secondary + '80',
                    borderColor: colors.secondary,
                    borderWidth: 1
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    y: {
                        beginAtZero: true,
                        title: {
                            display: true,
                            text: 'Percentage (%)'
                        }
                    }
                },
                plugins: {
                    title: {
                        display: true,
                        text: 'Enrollment Distribution: Grade 10 Growth (+2.9 pts), Grade 12 Decline (-2.3 pts)'
                    },
                    legend: {
                        display: true
                    }
                }
            }
        });

        // Chart 2: Combined Lead vs Enrollment Conversion (2024 vs 2025)
        const ctx2 = document.getElementById('conversionComparisonChart').getContext('2d');
        new Chart(ctx2, {
            type: 'line',
            data: {
                labels: ['Grade 6 & Below', 'Grade 7', 'Grade 8', 'Grade 9', 'Grade 10', 'Grade 11', 'Grade 12'],
                datasets: [{
                    label: '2024 Lead Creation',
                    data: [7.4, 8.0, 11.1, 15.7, 18.4, 19.5, 7.9],
                    borderColor: colors.accent + 'AA',
                    backgroundColor: colors.accent + '20',
                    fill: false,
                    tension: 0.1,
                    pointBackgroundColor: colors.accent,
                    pointBorderColor: colors.accent,
                    pointRadius: 5,
                    borderDash: [5, 5]
                }, {
                    label: '2024 Final Enrollments',
                    data: [6.6, 8.1, 10.4, 21.2, 22.1, 21.7, 9.9],
                    borderColor: colors.primary,
                    backgroundColor: colors.primary + '20',
                    fill: false,
                    tension: 0.1,
                    pointBackgroundColor: colors.primary,
                    pointBorderColor: colors.primary,
                    pointRadius: 6
                }, {
                    label: '2025 Lead Creation',
                    data: [9.0, 7.4, 11.0, 17.7, 20.3, 23.1, 11.6],
                    borderColor: colors.grade12 + 'AA',
                    backgroundColor: colors.grade12 + '20',
                    fill: false,
                    tension: 0.1,
                    pointBackgroundColor: colors.grade12,
                    pointBorderColor: colors.grade12,
                    pointRadius: 5,
                    borderDash: [5, 5]
                }, {
                    label: '2025 Final Enrollments',
                    data: [6.5, 6.7, 13.3, 21.0, 25.0, 19.9, 7.6],
                    borderColor: colors.secondary,
                    backgroundColor: colors.secondary + '20',
                    fill: false,
                    tension: 0.1,
                    pointBackgroundColor: colors.secondary,
                    pointBorderColor: colors.secondary,
                    pointRadius: 6
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    y: {
                        beginAtZero: true,
                        title: {
                            display: true,
                            text: 'Percentage (%)'
                        }
                    }
                },
                plugins: {
                    title: {
                        display: true,
                        text: 'Lead vs Enrollment Patterns: 2024 vs 2025 Comparison'
                    },
                    legend: {
                        position: 'bottom'
                    }
                }
            }
        });

        // Chart 3: Lead Trends 2024 vs 2025 (Simplified - removed "Other Grades")
        const ctx3 = document.getElementById('leadTrendsChart').getContext('2d');
        new Chart(ctx3, {
            type: 'bar',
            data: {
                labels: ['Grades 9-11', 'Grade 9 & Below (Target Demo)'],
                datasets: [{
                    label: '2024 Leads',
                    data: [53.5, 43.4],
                    backgroundColor: [colors.primary + '80', colors.secondary + '80'],
                    borderColor: [colors.primary, colors.secondary],
                    borderWidth: 2
                }, {
                    label: '2025 Leads',
                    data: [61.0, 45.1],
                    backgroundColor: [colors.primary, colors.secondary],
                    borderColor: [colors.primary, colors.secondary],
                    borderWidth: 2
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    y: {
                        beginAtZero: true,
                        max: 70,
                        title: {
                            display: true,
                            text: 'Percentage (%)'
                        }
                    }
                },
                plugins: {
                    title: {
                        display: true,
                        text: 'Lead Generation Focus: Strategic Segments 2024 → 2025'
                    },
                    legend: {
                        position: 'bottom'
                    }
                }
            }
        });

        // Chart 3: Complete - all charts rendered above
    </script>

